<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="181dp"
        android:layout_height="121dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp"
        tools:srcCompat="@tools:sample/avatars"
        android:contentDescription="Logo 1" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="181dp"
        android:layout_height="121dp"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="16dp"
        tools:srcCompat="@tools:sample/avatars"
        android:contentDescription="Logo 2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="408dp"
        android:layout_height="75dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="656dp"
        tools:srcCompat="@tools:sample/avatars"
        android:contentDescription="ad" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="261dp"
        android:layout_height="261dp"
        android:contentDescription="Main image"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="161dp"
        tools:srcCompat="@drawable/ic_launcher_background" />

    <ImageButton
        android:id="@+id/flopper"
        android:layout_width="59dp"
        android:layout_height="68dp"
        android:adjustViewBounds="true"
        android:background="@null"
        android:contentDescription="Save button"
        android:scaleType="fitCenter"
        android:src="@drawable/flopperdisk"
        tools:layout_editor_absoluteX="336dp"
        tools:layout_editor_absoluteY="161dp" />

    <Button
        android:id="@+id/getit"
        android:layout_width="382dp"
        android:layout_height="64dp"
        android:adjustViewBounds="true"
        android:text="GET IT"
        android:contentDescription="Submit"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="524dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="385dp"
        android:layout_height="51dp"
        android:hint="Input image prompt here"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="445dp"
        tools:ignore="MissingConstraints">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/editTextInput"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="text" />
    </com.google.android.material.textfield.TextInputLayout>




    </androidx.constraintlayout.widget.ConstraintLayout>